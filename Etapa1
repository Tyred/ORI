#include <iostream>
#include <string.h>

using namespace std;

class Etapa1
{
public:
    void ct();
    void rt();
    void ciA();
    void ciH();
    void ri();
    void gi();
    void eb();
    void at();
    void lt();
    void ir();
    void brN();
    void brU();
    void ar();
    void rr();
};

int main()
{
    char palavra[100];

    gets(palavra);

    if(palavra == 'CT')
    {
        cout << "Arquivo criado!"
    }

    if(palavra == 'RT')
    {
        cout << "Arquivo apagado e Metadados removidos!"
    }

    if(palavra == 'CI A')
    {
        cout << "Indice criado!"
    }

    if(palavra == 'CI H')
    {
        cout << "Indice criado!"
    }

    if(palavra == 'RI')
    {
        cout << "Indice removido!"
    }

    if(palavra == 'GI')
    {
        cout << "Indice gerado!"
    }

    if(palavra == 'EB')
    {
        cout << "Interpretacao encerrada"
    }

    if(palavra == 'AT')
    {
        cout << "Resumos apresentados!"
    }

    if(palavra == 'LT')
    {
        cout << "Nomes listados!"
    }

    if(palavra == 'IR')
    {
        cout << "Registro inserido!"
    }

    if(palavra == 'BR N')
    {
        cout << "Busca realizada!"
    }

    if(palavra == 'BR U')
    {
        cout << "Busca realizada!"
    }

    if(palavra == 'AR')
    {
        cout << "Valores apresentados!"
    }

    if(palavra == 'RR')
    {
        cout << "Registros removidos!"
    }
}

